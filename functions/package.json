{
    "name": "functions",
    "description": "Cloud Functions for Shape of the Day AI Agents",
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "firebase deploy --only functions",
        "genkit:dev": "genkit start -- npx tsx --watch src/index.ts"
    },
    "engines": {
        "node": "20"
    },
    "main": "lib/index.js",
    "dependencies": {
        "firebase-admin": "^12.0.0",
        "firebase-functions": "^5.0.0",
        "@genkit-ai/ai": "^0.5.0",
        "@genkit-ai/core": "^0.5.0",
        "@genkit-ai/firebase": "^0.5.0",
        "@genkit-ai/googleai": "^0.5.0",
        "@genkit-ai/vertexai": "^0.5.0",
        "pdf-parse": "^1.1.1"
    },
    "devDependencies": {
        "typescript": "^5.4.0",
        "tsx": "^4.0.0"
    },
    "private": true
}