{
    "name": "functions",
    "description": "Cloud Functions for Shape of the Day AI Agents",
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "firebase deploy --only functions",
        "genkit:dev": "genkit start -- npx tsx --watch src/index.ts"
    },
    "engines": {
        "node": "20"
    },
    "main": "lib/index.js",
    "dependencies": {
        "@genkit-ai/ai": "^1.26.0",
        "@genkit-ai/core": "^1.26.0",
        "@genkit-ai/firebase": "^1.26.0",
        "@genkit-ai/googleai": "^1.26.0",
        "@genkit-ai/vertexai": "^1.26.0",
        "@google-cloud/bigquery": "^8.1.1",
        "@google-cloud/firestore": "^7.11.6",
        "@google-cloud/pubsub": "^5.2.0",
        "@google-cloud/storage": "^7.18.0",
        "@google-cloud/vertexai": "^1.10.0",
        "firebase-admin": "^12.7.0",
        "firebase-functions": "^7.0.1",
        "pdf-parse": "^1.1.1"
    },
    "devDependencies": {
        "tsx": "^4.0.0",
        "typescript": "^5.4.0"
    },
    "private": true
}
