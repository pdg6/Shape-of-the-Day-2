{
    "name": "functions",
    "description": "Cloud Functions for Shape of the Day AI Agents",
    "scripts": {
        "build": "tsc",
        "serve": "npm run build && firebase emulators:start --only functions",
        "shell": "npm run build && firebase functions:shell",
        "deploy": "firebase deploy --only functions",
        "genkit:dev": "genkit start -- npx tsx --watch src/index.ts"
    },
    "engines": {
        "node": "20"
    },
    "type": "module",
    "main": "lib/index.js",
    "dependencies": {
        "@genkit-ai/ai": "^1.27.0",
        "@genkit-ai/core": "^1.27.0",
        "@genkit-ai/firebase": "^1.27.0",
        "@genkit-ai/google-genai": "^1.27.0",
        "@genkit-ai/vertexai": "^1.27.0",
        "@google-cloud/bigquery": "^8.1.1",
        "@google-cloud/firestore": "^7.11.6",
        "@google-cloud/pubsub": "^5.2.0",
        "@google-cloud/storage": "^7.18.0",
        "@google-cloud/vertexai": "^1.10.0",
        "@google/genai": "^1.34.0",
        "dotenv": "^17.2.3",
        "firebase-admin": "^12.7.0",
        "firebase-functions": "^6.1.1",
        "genkit": "^1.27.0",
        "pdf-parse": "^1.1.4",
        "youtube-transcript": "^1.2.1",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "tsx": "^4.0.0",
        "typescript": "^5.4.0"
    },
    "private": true
}
